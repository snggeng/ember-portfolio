<!-- About Section -->
<section id="skills">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">Skills</h2>
                <svg version="1.0" id="Layer_4" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                	 width="200px" height="200px" viewBox="0 0 64 64" enable-background="new 0 0 64 64" xml:space="preserve">
                <circle fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" cx="56" cy="34" r="3"/>
                <circle fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" cx="22" cy="4" r="3"/>
                <circle fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" cx="40" cy="50" r="3"/>
                <circle fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" cx="7" cy="31" r="3"/>
                <line fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" x1="8" y1="28" x2="20" y2="6"/>
                <line fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" x1="43" y1="48" x2="54" y2="36"/>
                <rect x="1" y="57" fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" width="62" height="6"/>
                <line fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" x1="24" y1="6" x2="38" y2="47"/>
                </svg>

                <h3 class="section-subheading text-muted">JS and Ruby mostly.</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <ul class="timeline">
                    <li>
                        <div class="timeline-image">
                          <img class="img-circle img-responsive" src="img/about/1.jpg" alt="">
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>FRONTEND</h4>
                                <h4 class="subheading">Languages and Frameworks</h4>
                            </div>
                            <div class="timeline-body">
                              <p class="text-muted">Where all the magic happens</p>
                              {{#each model as |profile|}}
                                {{#each profile.skills.frontend as |frontend|}}
                                <i class="{{frontend.icon}}" style="font-size:7vw"></i>
                                {{/each}}
                              {{/each}}
                            </div>
                        </div>
                    </li>
                    <li class="timeline-inverted">
                        <div class="timeline-image">
                            <img class="img-circle img-responsive" src="img/about/2.jpg" alt="">
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>BACKEND</h4>
                                <h4 class="subheading">Languages and Databases</h4>
                            </div>
                            <div class="timeline-body">
                                <p class="text-muted">Fluent in SQL and noSQL databases</p>
                                {{#each model as |profile|}}
                                  {{#each profile.skills.backend as |backend|}}
                                  <i class="{{backend.icon}}" style="font-size:7vw"></i>
                                  {{/each}}
                                {{/each}}
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="timeline-image">
                            <img class="img-circle img-responsive" src="img/about/3.jpg" alt="">
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>DEPLOYMENT</h4>
                                <h4 class="subheading">Bringing Ideas Into Production</h4>
                            </div>
                            <div class="timeline-body">
                                <p class="text-muted">Test-driven Development</p>
                                {{#each model as |profile|}}
                                  {{#each profile.skills.deployment as |deployment|}}
                                  <i class="{{deployment.icon}}" style="font-size:7vw"></i>
                                  {{/each}}
                                {{/each}}
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</section>
