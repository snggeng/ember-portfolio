
<!-- Services Section -->
<section id="services">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <!-- <object id="Layer_1" type="image/svg+xml" data="img/svg/basic_alarm.svg" onclick="clock.reset().play()" class="container-fluid text-center svg" style="height: 200px;"></object>   -->
                <h2 class="section-heading">Services</h2>
                <svg version="1.1" id="Layer_2" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                   width="200px" height="200px" viewBox="0 0 64 64" enable-background="new 0 0 64 64" xml:space="preserve">
                <g>
                  <rect x="1" y="18" fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" width="62" height="36"/>
                </g>
                <line fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" x1="1" y1="30" x2="63" y2="30"/>
                <polyline fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" points="27,30 27,36 37,36 37,30 "/>
                <path fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" d="M23,18c0,0,0-8,9-8s9,8,9,8"/>
                </svg>
                {{#each model as |profile|}}
                <h3 class="section-subheading text-muted">{{profile.description}}</h3>
                {{/each}}
            </div>
        </div>
        <div class="row text-center">
          {{#each model as |profile|}}
          {{#each profile.services as |service|}}
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="{{service.image}} fa fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">
                    <strong>{{service.category}}</strong>
                </h4>
                <p class="text-muted">{{service.description}}</p>
            </div>
            {{/each}}
          {{/each}}
        </div>
    </div>
</section>
