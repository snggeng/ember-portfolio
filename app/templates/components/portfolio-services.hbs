
<!-- Services Section -->
<section id="services">
    <div class="container">
        <div class="row">
          <object id="Layer_1" type="image/svg+xml" data="img/svg/lightbulb.svg" style="height: 200px;"></object>
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">Services</h2>
                {{#each model as |profile|}}
                <h3 class="section-subheading text-muted">{{profile.description}}</h3>
                {{/each}}
            </div>
        </div>
        <div class="row text-center">
          {{#each model as |profile|}}
          {{#each profile.services as |service|}}
            <div class="col-md-4">
                <span class="fa-stack fa-4x">
                    <i class="fa fa-circle fa-stack-2x text-primary"></i>
                    <i class="{{service.image}} fa fa-stack-1x fa-inverse"></i>
                </span>
                <h4 class="service-heading">
                    <strong>{{service.category}}</strong>
                </h4>
                <p class="text-muted">{{service.description}}</p>
            </div>
            {{/each}}
          {{/each}}
        </div>
    </div>
</section>
