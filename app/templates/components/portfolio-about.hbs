<!-- About Section -->
<section id="about" class="bg-light-gray">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">About</h2>
                <svg version="1.1" id="Layer_6" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                	 width="200px" height="200px" viewBox="0 0 64 64" enable-background="new 0 0 64 64" xml:space="preserve">
                <path fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" d="M21,40v5h22l0.001-5.107C49,36.195,53,29.564,53,22
                	c0-11.598-9.402-21-21-21s-21,9.402-21,21C11,29.565,14.998,36.304,21,40z"/>
                <line fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" x1="28" y1="45" x2="25" y2="25"/>
                <line fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" x1="36" y1="45" x2="39" y2="25"/>
                <polyline fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" points="25,26 29,29 32,26 35,29 39,26 "/>
                <rect x="21" y="45" fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" width="22" height="6"/>
                <rect x="23" y="51" fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" width="18" height="6"/>
                <rect x="25" y="57" fill="none" stroke="#fed136" stroke-width="2" stroke-miterlimit="10" width="14" height="6"/>
                </svg>
                <h3 class="section-subheading text-muted">Singaporean Developer, Entrepreneur &amp; International student at Duke University</h3>
            </div>
        </div>
        <div class="row">
            <div class="container-fluid text-center">
              <!-- I'm a comment  -->
                <div class="team-member">
                    <img src="profile-small.jpg" class="img-responsive img-circle" alt="">
                    <h4>Geng Sng</h4>
                    <p class="text-muted">Web Developer</p>
                    <ul class="list-inline social-buttons">
                        <li><a href="https://twitter.com/snggeng"><i class="fa fa-twitter"></i></a>
                        </li>
                        <li><a href="https://www.facebook.com/sng.geng#"><i class="fa fa-facebook"></i></a>
                        </li>
                        <li><a href="https://github.com/snggeng"><i class="fa fa-github"></i></a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="row text-center">
            {{#each model as |profile|}}
              {{#each profile.hats as |hat|}}

                <div class="col-md-4">
                  <span class="fa-stack fa-4x">
                      <i class="fa fa-circle fa-stack-2x text-primary"></i>
                      <i class="{{hat.image}} fa-stack-1x fa-inverse"></i>
                  </span>
                <!-- </div>

                <div class="col-md-4"> -->
                  <h4><strong>{{hat.category}}</strong></h4>
                  <p class="text-muted">{{hat.description}}</p>
                </div>

              {{/each}}
            {{/each}}
          </div>
<!-- Education -->
      </div>
      <br><br><br>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Education</h2>
                </div>
            </div>
              <div class="row text-center">
              {{#each model as |profile|}}
                {{#each profile.education as |school|}}

                  <div class="col-md-4">
                    <img src="{{school.image}}" class="img-responsive img-circle center-block"/>
                  <!-- </div>

                  <div class="col-md-4"> -->
                    <h4><strong>{{school.name}}</strong></h4>
                    <p class="text-muted">{{school.description}}</p>
                  </div>

                {{/each}}
              {{/each}}
              </div>
        </div>
        <!-- Resume -->
        <br><br><br>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Resume</h2>
                </div>
            </div>
              <div class="row text-center">
                <div class="col-lg-12 text-center">
                    <div id="success"></div>
                    <!-- {{#link-to 'resume' type="button" class="btn btn-xl"}} Get Resume {{/link-to}} -->
                    <a type="button" class="btn btn-xl" href="/geng-sng-resume.pdf" download> Get Resume </a>
                </div>
              </div>
        </div>
    </div>
</section>
